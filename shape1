package assignment4;

public class shape1 {

	public static void main(String[] args) 
	{
		CalculateArea ob = new CalculateArea();
		ob.area(5);
		ob.area(8);
	}
}
class CalculateArea
{
	void area(float x)
	{
		System.out.println("The area of square is "+Math.pow(x,2)+"sq units");
	}
	void area(double x)
	{
		double z=3.14*x*x;
		System.out.println("the area of circle is"+z+" sq units");

	}

}
